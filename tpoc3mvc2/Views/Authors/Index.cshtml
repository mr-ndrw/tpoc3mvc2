@model IEnumerable<tpoc3mvc2.Models.Authors>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th>
            @Html.Raw("Name")
        </th>
        <th></th>
